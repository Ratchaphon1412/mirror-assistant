{
    "version": 2,
    "builds": [
        {
            "src": "asgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "asgi.py"
        },
        {
            "src": "/(.*)",
            "dest": "asgi.py"
        },
        {
            "src": "/ws/security/",
            "dest": "asgi.py",
            "protocols": [
                "websocket"
            ]
        }
    ]
}